<link rel="stylesheet" href="/css/signin.css">
<div class="signin-container">
    <h2>Sign In</h2>
    <form class="signin-form" action="/users/login" method="post" >
        <input type="email" class="form-input" placeholder="Email Address" required name="email">
        <input type="password" class="form-input" placeholder="Password" required name="password">
        <button type="submit" class="form-button">Sign In</button>
    </form>
</div>